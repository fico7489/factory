{
    "name": "fico7489/factory",
    "authors": [
        {
            "name": "Filip Horvat",
            "email": "fico7489@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "7.*",
        "symfony/runtime": "7.*",
        "symfony/yaml": "7.*",
        "symfony/flex": "2.*",
        "symfony/dotenv": "7.*",
        "doctrine/doctrine-bundle": "2.*",
        "api-platform/core": "3.*",
        "doctrine/orm": "3.0.*",
        "doctrine/dbal": "3.*",
        "doctrine/doctrine-migrations-bundle": "3.*",
        "guzzlehttp/guzzle": "7.*",
        "symfony/twig-bundle": "7.*",
        "symfony/asset": "7.*",
        "ramsey/uuid-doctrine": "2.*",
        "symfony/monolog-bundle": "3.*",
        "beberlei/doctrineextensions": "1.*",
        "symfony/password-hasher": "7.*",
        "symfony/security-bundle": "7.*",
        "lexik/jwt-authentication-bundle": "^2.20"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "friendsofphp/php-cs-fixer": "3.*",
        "phpstan/phpstan": "1.*",
        "symfony/browser-kit": "7.*",
        "symfony/http-client": "7.*",
        "jdorn/sql-formatter": "^1.2",
        "dama/doctrine-test-bundle": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
